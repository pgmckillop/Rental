/****** Object:  StoredProcedure [dbo].[spVehicle_GetAll]    Script Date: 02/04/2020 11:53:24 ******/
SET ANSI_NULLS ON
GO

SET QUOTED_IDENTIFIER ON
GO

CREATE PROC [dbo].[spVehicle_GetAll]

AS

BEGIN
	SELECT id, Registration, DateOnFleet, Gearbox, MaxLadenWeight
	FROM dbo.Vehicle
END

GO


